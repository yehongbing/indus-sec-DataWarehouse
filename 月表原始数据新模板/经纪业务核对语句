
--经纪业务约定购回月报
	--约定购回总资产_期末 
	SELECT SUM(GUAR_SECU_MVAL_FINAL),'DM.T_AST_EMPCUS_APPTBUYB_M_D' FROM DM.T_AST_EMPCUS_APPTBUYB_M_D
	UNION ALL
	SELECT SUM(APPTBUYB_TOTAST_FINAL),'DM.T_BRKBIS_APPTBUYB_M' FROM DM.T_BRKBIS_APPTBUYB_M;
	
	--约定购回余额_期末
	SELECT SUM(APPTBUYB_BAL_FINAL),'DM.T_AST_EMPCUS_APPTBUYB_M_D' FROM DM.T_AST_EMPCUS_APPTBUYB_M_D
	UNION ALL
	SELECT SUM(APPTBUYB_BAL_FINAL),'DM.T_BRKBIS_APPTBUYB_M' FROM DM.T_BRKBIS_APPTBUYB_M;
	
	--约定购回总资产_月日均
	SELECT SUM(GUAR_SECU_MVAL_MDA),'DM.T_AST_EMPCUS_APPTBUYB_M_D' FROM DM.T_AST_EMPCUS_APPTBUYB_M_D
	UNION ALL
	SELECT SUM(APPTBUYB_TOTAST_MDA),'DM.T_BRKBIS_APPTBUYB_M' FROM DM.T_BRKBIS_APPTBUYB_M;
	
	--约定购回余额_月日均
	SELECT SUM(APPTBUYB_BAL_MDA),'DM.T_AST_EMPCUS_APPTBUYB_M_D' FROM DM.T_AST_EMPCUS_APPTBUYB_M_D
	UNION ALL
	SELECT SUM(APPTBUYB_BAL_MDA),'DM.T_BRKBIS_APPTBUYB_M' FROM DM.T_BRKBIS_APPTBUYB_M;

	--约定购回总资产_年日均  
	SELECT SUM(GUAR_SECU_MVAL_YDA),'DM.T_AST_EMPCUS_APPTBUYB_M_D' FROM DM.T_AST_EMPCUS_APPTBUYB_M_D
	UNION ALL
	SELECT SUM(APPTBUYB_TOTAST_YDA),'DM.T_BRKBIS_APPTBUYB_M' FROM DM.T_BRKBIS_APPTBUYB_M;

	--约定购回余额_年日均  
	SELECT SUM(APPTBUYB_BAL_YDA),'DM.T_AST_EMPCUS_APPTBUYB_M_D' FROM DM.T_AST_EMPCUS_APPTBUYB_M_D
	UNION ALL
	SELECT SUM(APPTBUYB_BAL_YDA),'DM.T_BRKBIS_APPTBUYB_M' FROM DM.T_BRKBIS_APPTBUYB_M;

	--约定购回净佣金_月累计
	SELECT SUM(APPTBUYB_NET_CMS_MTD),'DM.T_EVT_EMPCUS_CRED_INCM_M_D' FROM DM.T_EVT_EMPCUS_CRED_INCM_M_D
	UNION ALL
	SELECT SUM(APPTBUYB_NET_CMS_MTD),'DM.T_BRKBIS_APPTBUYB_M' FROM DM.T_BRKBIS_APPTBUYB_M;